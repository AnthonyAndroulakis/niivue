{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-docusaurus-theme"],
  "entryFileName": "index",
  "readme": "none",
  "enumMembersFormat": "table",
  "parametersFormat": "table",
  "classPropertiesFormat": "table",
  "propertyMembersFormat": "table",
  "interfacePropertiesFormat": "table",
  "formatWithPrettier": true,
  "typeDeclarationFormat": "table",
  "indexFormat": "table",
  "useCodeBlocks": true,
  "expandObjects": false,
  "expandParameters": true,
  "gitRevision": "main",
  "out": "./devdocs",
  "name": "API",
  "includeVersion": true,
  "basePath": "src",
  "entryPoints": [
    "src/niivue/index.ts",
    "src/nvimage/index.ts",
    "src/colortables.ts",
    "src/nvconnectome.ts",
    "src/nvdocument.ts",
    "src/nvlabel.ts",
    "src/nvmesh-loaders.ts",
    "src/nvmesh-types.ts",
    "src/nvmesh-utilities.ts",
    "src/nvmesh.ts",
    "src/nvutilities.ts",
    "src/shader.ts",
    "src/types.ts"
  ],
  "excludeInternal": false,
  "excludeNotDocumented": false,
  "router": "member",
  "cleanOutputDir": true
}
